{
  "name": "on-botics.crafters-up.api-canvas",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "tsc --build",
    "clean": "tsc --build --clean",
    "start": "node app",
    "dev": "yarn build && node app",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "description": "OnBotics.CraftersUp.ApiCanvas",
  "author": {
    "name": ""
  },
  "main": "app.js",
  "dependencies": {
    "@types/node-fetch": "2",
    "body-parser": "^1.20.2",
    "canvas": "2.11.2",
    "cors": "^2.8.5",
    "debug": "^2.2.0",
    "dotenv": "^16.0.3",
    "express": "^4.14.0",
    "fabric": "5.3.0",
    "makerjs": "^0.18.1",
    "morgan": "^1.10.0",
    "opentype.js": "^1.3.4",
    "node-fetch": "^2.7.0",
    "pug": "^2.0.0-rc.3",
    "uuid": "^9.0.0"
  },
  "keywords": [
    "fonts",
    "canvas",
    "fabric.js",
    "node-canvas",
    "dynamic-fonts",
    "registerFont"
  ],
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/debug": "0.0.30",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.0.37",
    "@types/express-serve-static-core": "^4.0.50",
    "@types/jest": "^29.5.0",
    "@types/mime": "^1.3.1",
    "@types/morgan": "^1.9.4",
    "@types/node": "^14.14.7",
    "@types/serve-static": "^1.7.32",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "eslint": "^8.21.0",
    "jest": "^29.5.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "typescript": "^4.5.2"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "plugins": [
      "@typescript-eslint"
    ]
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/test"
    ],
    "testMatch": [
      "**/*.spec.ts"
    ],
    "collectCoverageFrom": [
      "services/**/*.ts",
      "routes/**/*.ts",
      "middleware/**/*.ts",
      "!**/*.spec.ts"
    ]
  }
}