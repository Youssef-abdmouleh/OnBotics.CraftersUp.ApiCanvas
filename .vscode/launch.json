{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug API Canvas",
      "program": "${workspaceFolder}/OnBotics.CraftersUp.ApiCanvas/app.js",
      "cwd": "${workspaceFolder}/OnBotics.CraftersUp.ApiCanvas",
      "preLaunchTask": "npm: build",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "env": {
        "NODE_ENV": "development"
      },
      "console": "integratedTerminal",
      "outputCapture": "std"
    },
    {
      "type": "node",
      "request": "attach",
      "name": "Attach to Process",
      "port": 9229,
      "restart": true,
      "skipFiles": [
        "<node_internals>/**"
      ]
    }
  ]
}